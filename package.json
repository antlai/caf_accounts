{
    "name": "caf_accounts",
    "description": "Cloud Assistants accounts management",
    "version": "0.2.7",
    "author": "Antonio Lain <antlai@cafjs.com>",
    "dependencies": {
        "caf_core": "^0.2.0",
        "caf_srp" : "^0.2.0",
        "caf_cli": "^0.2.0",
        "caf_transport": "^0.2.0",
        "caf_security" : "^0.2.0",
        "react" : "^0.13.0",
        "react-bootstrap" : "^0.20.3",
        "flux": "^2.0.2",
        "keymirror": "^0.1.0",
        "eventemitter2" : "^0.4.14"
    },
    "devDependencies": {
        "nodeunit" : ">=0.9",
        "jwt-decode" : "^1.1.0",
        "aliasify": "^1.7.2",
        "varify": "^0.1.1",
        "browserify": "^9.0.3",
        "uglify-js" : "^2.4.0",
        "srp-shim" :"^0.2.0"
    },
    "aliasify": {
        "aliases" : {
            "srp": "srp-shim"
        }
    },
    "scripts": {
        "start" : "node ./index.js",
        "test": "node ./node_modules/.bin/nodeunit",
        "build-js" : "browserify  -d -g varify -g aliasify -r ./public/js/app.js:app -o public/js/build.js",
        "build-min-js" : "export NODE_ENV=production; browserify -g varify -g aliasify  -r ./public/js/app.js:app | uglifyjs > public/js/build.min.js",
        "build": "npm run build-js && npm run build-min-js"
    },
   "main": "index",
  "engines": { "node": ">= 0.10.29" }
}
